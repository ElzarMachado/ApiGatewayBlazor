@page "/counter"

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">like</button>

<p role="status">Current count: @currentCount2</p>
<button class="btn btn-primary" @onclick="IncrementCount2">dis</button>


@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;

    }

    private int currentCount2 = 0;

    private void IncrementCount2()
    {
        currentCount2++;

    }
}

